{
  "ConnectionStrings": {
    "DefaultConnection": "Host={HOST}:3535; Database=wordsearch; User Id={DB_USERNAME}; Password={DB_PASSWORD}; Pooling=true; SearchPath=wordsearch;"
  },
  "JwtSettings": {
    "Issuer": "https://wordsearchbattle.api.edenmor.com",
    "Audience": "https://wordsearchbattle.api.edenmor.com",
    "Key": "{JWT_KEY}"
  },
  "Database": {
    "PlayerSchemaName": "playerData",
    "GameSchemaName": "gameData",
    "DockerDBHost": "word_search_db",
    "DirectServerDBHost": "edenmor.com"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}